#===============================================================================
# Latex Asymptom
#===============================================================================
# Compile
#-------------------------------------------------------------------------------
asytex := asy
#-------------------------------------------------------------------------------
src_dir := .
#-------------------------------------------------------------------------------
asy_fnm := t01
#-------------------------------------------------------------------------------

#-------------------------------------------------------------------------------
# PHONY
#-------------------------------------------------------------------------------
.PHONY: all
#-------------------------------------------------------------------------------
# Target
#-------------------------------------------------------------------------------
all:
	${asytex} ${src_dir}/${asy_fnm}.asy
#-------------------------------------------------------------------------------
# Clean
#-------------------------------------------------------------------------------
clean:
	@rm -f ${asy_fnm}.pdf
	@rm -f ${asy_fnm}.eps
	@rm -f ${asy_fnm}.png
#===============================================================================
# vim:ft=make:nu:nowrap
